{
  "name": "MLVET",
  "description": "Machine Learning Video Editor Toolkit",
  "main": "./dist/main/main.js",
  "author": {
    "name": "MLVET Developers",
    "url": "https://github.com/MLVETDevelopers"
  },
  "version": "0.1.0",
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "build": "concurrently \"yarn build:main\" \"yarn build:renderer\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "lint": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "pre:package": "node ./tools/prepackage.js",
    "package": "yarn pre:package && ts-node ./.erb/scripts/clean.js dist && yarn build && electron-builder build --publish never",
    "postinstall": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
    "start": "ts-node ./.erb/scripts/check-port-in-use.js && yarn start:renderer",
    "start:main": "cross-env NODE_ENV=development electron -r ts-node/register/transpile-only ./src/main/main.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "test": "jest",
    "prepare": "husky install",
    "gen": "ts-node ./tools/gencode.ts && prettier ./src/**/*.ts --write && prettier ./src/**/*.js --write",
    "install:slow": "yarn config set network-timeout 100000 && yarn",
    "publish:linux": "yarn pre:package && yarn run build && electron-builder -l --publish always",
    "publish:mac": "yarn pre:package && yarn run build && electron-builder -m  --x64 --arm64 --publish always",
    "publish:windows": "yarn pre:package && yarn run build && electron-builder -w --x64 --ia32 --publish always"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --fix --cache",
      "prettier --ignore-path .eslintignore --write"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --write"
    ]
  },
  "build": {
    "productName": "MLVET",
    "copyright": "Copyright Â© 2022 ${author}",
    "extraMetadata": {
      "//": "--- Prop 'version' will be automatically set by electron-builder ---",
      "version": "0.0.2"
    },
    "appId": "edu.monash.mlvet",
    "asar": true,
    "asarUnpack": [
      "**\\*.{node,dll}",
      "node_modules/ffmpeg-static/bin/${os}/${arch}/ffmpeg",
      "node_modules/ffmpeg-static/index.js",
      "node_modules/ffmpeg-static/package.json"
    ],
    "files": [
      "dist",
      "node_modules",
      "package.json",
      "node_modules/ffprobe-static/index.js",
      "node_modules/ffprobe-static/package.json"
    ],
    "extraResources": [
      "./assets/**",
      {
        "from": "node_modules/ffmpeg-static/",
        "to": "ffmpeg-static/"
      },
      {
        "from": "node_modules/ffprobe-static/",
        "to": "ffprobe-static/"
      }
    ],
    "afterSign": ".erb/scripts/notarize.js",
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "files": [
        "!node_modules/ffmpeg-static/bin/linux${/*}",
        "!node_modules/ffmpeg-static/bin/win32${/*}",
        "!node_modules/ffprobe-static/bin/linux${/*}",
        "!node_modules/ffprobe-static/bin/win32${/*}"
      ],
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "files": [
        "!node_modules/ffmpeg-static/bin/linux${/*}",
        "!node_modules/ffmpeg-static/bin/darwin${/*}",
        "!node_modules/ffprobe-static/bin/linux${/*}",
        "!node_modules/ffprobe-static/bin/darwin${/*}"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development",
      "files": [
        "!node_modules/ffmpeg-static/bin/darwin${/*}",
        "!node_modules/ffmpeg-static/bin/win32${/*}",
        "!node_modules/ffprobe-static/bin/darwin${/*}",
        "!node_modules/ffprobe-static/bin/win32${/*}"
      ]
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "publish": {
      "provider": "github",
      "owner": "MLVETDevelopers",
      "repo": "mlvet",
      "releaseType": "release",
      "vPrefixedTagName": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MLVETDevelopers/mlvet.git"
  },
  "jest": {
    "testURL": "http://localhost/",
    "testEnvironment": "jsdom",
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "@fontsource/*": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules",
      "src"
    ],
    "testPathIgnorePatterns": [
      "release/app/dist"
    ]
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.4",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/jest": "^27.0.3",
    "@types/node": "17.0.5",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "@types/react-text-truncate": "^0.14.1",
    "@types/terser-webpack-plugin": "^5.0.4",
    "@types/uuid": "^8.3.4",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "^4.1.2",
    "change-case": "^4.1.2",
    "concurrently": "^6.5.1",
    "core-js": "^3.20.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "detect-port": "^1.3.0",
    "electron": "^18.0.4",
    "electron-builder": "^23.3.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-notarize": "^1.1.1",
    "electron-rebuild": "^3.2.5",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-erb": "^4.0.3",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-compat": "^4.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.5",
    "koffi": "^2.1.1",
    "lint-staged": "^12.1.4",
    "mini-css-extract-plugin": "^2.4.5",
    "nodemon": "^2.0.16",
    "prettier": "^2.5.1",
    "react-refresh": "^0.11.0",
    "react-refresh-typescript": "^2.0.3",
    "react-test-renderer": "^17.0.2",
    "redux-devtools-extension": "^2.13.9",
    "rimraf": "^3.0.2",
    "sass": "^1.45.1",
    "sass-loader": "^12.4.0",
    "socket.io-mock": "^1.3.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^1.4.1",
    "@fontsource/rubik": "^4.5.6",
    "@mui/icons-material": "^5.5.1",
    "@mui/lab": "^5.0.0-alpha.101",
    "@mui/material": "^5.5.1",
    "@react-hook/debounce": "^4.0.0",
    "@react-hook/mouse-position": "^4.1.3",
    "@react-hook/throttle": "^2.2.0",
    "axios": "^0.27.2",
    "buffer": "^6.0.3",
    "child_process": "^1.0.2",
    "dotenv": "^16.0.0",
    "electron-debug": "^3.2.0",
    "electron-log": "^4.4.6",
    "electron-updater": "^4.6.5",
    "express": "^4.18.1",
    "extract-zip": "^2.0.1",
    "ffmpeg-static": "^5.0.2",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^10.1.0",
    "history": "^5.3.0",
    "node-fetch": "2.6.6",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-text-truncate": "^0.19.0",
    "redux": "^4.1.2",
    "socket.io-client": "^4.5.0",
    "uuid": "^8.3.2",
    "vimond-replay": "^3.3.0",
    "wavefile": "^11.0.0",
    "wget-improved": "^3.3.1"
  },
  "devEngines": {
    "node": "16.x",
    "npm": ">=8.x"
  },
  "browserslist": [],
  "prettier": {
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ],
    "singleQuote": true
  }
}
